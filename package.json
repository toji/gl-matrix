{
  "version": "4.0.0-beta.3",
  "name": "gl-matrix",
  "description": "Javascript Matrix and Vector library for High Performance 3D web apps",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "imports": {
    "#gl-matrix": "./src/index.ts",
    "#gl-matrix/classic": "./src/classic/index.ts",
    "#gl-matrix/common": "./src/common/index.ts",
    "#gl-matrix/f64": "./src/f64/index.ts",
    "#gl-matrix/swizzle": "./src/swizzle/index.ts",
    "#gl-matrix/swizzle/f64": "./src/swizzle/f64/index.ts",
    "#gl-matrix/types": "./src/types/index.ts",
    "#gl-matrix/util": "./src/util/index.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./classic": {
      "types": "./dist/classic/index.d.mts",
      "import": "./dist/classic/index.js",
      "require": "./dist/classic/index.cjs"
    },
    "./common": {
      "types": "./dist/common/index.d.mts",
      "import": "./dist/common/index.js",
      "require": "./dist/common/index.cjs"
    },
    "./f64": {
      "types": "./dist/f64/index.d.mts",
      "import": "./dist/f64/index.js",
      "require": "./dist/f64/index.cjs"
    },
    "./swizzle": {
      "types": "./dist/swizzle/index.d.mts",
      "import": "./dist/swizzle/index.js",
      "require": "./dist/swizzle/index.cjs"
    },
    "./swizzle/f64": {
      "types": "./dist/swizzle/f64/index.d.mts",
      "import": "./dist/swizzle/f64/index.js",
      "require": "./dist/swizzle/f64/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.mts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./types/swizzle": {
      "types": "./dist/types/swizzle/index.d.mts",
      "import": "./dist/types/swizzle/index.js",
      "require": "./dist/types/swizzle/index.cjs"
    },
    "./types/swizzle/f64": {
      "types": "./dist/types/swizzle/f64/index.d.mts",
      "import": "./dist/types/swizzle/f64/index.js",
      "require": "./dist/types/swizzle/f64/index.cjs"
    },
    "./util": {
      "types": "./dist/util/index.d.mts",
      "import": "./dist/util/index.js",
      "require": "./dist/util/index.cjs"
    }
  },
  "homepage": "https://glmatrix.net",
  "bugs": {
    "url": "https://github.com/toji/gl-matrix/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toji/gl-matrix.git"
  },
  "contributors": [
    {
      "name": "Brandon Jones",
      "email": "tojiro@gmail.com"
    },
    {
      "name": "Colin MacKenzie IV",
      "email": "sinisterchipmunk@gmail.com"
    }
  ],
  "files": [
    "/dist"
  ],
  "keywords": [
    "vector",
    "matrix",
    "library",
    "webgl",
    "webgpu"
  ],
  "scripts": {
    "build": "npm run gen64 && tsc --project tsconfig-build.json && node scripts/bundle.js",
    "build-all": "npm run build && npm run types && npm run docs",
    "docs": "typedoc-pkg --api-link es",
    "gen64": "node scripts/gen-f64.js",
    "test": "vitest run",
    "test-ui": "vitest --ui --coverage",
    "types": "esm-d-ts gen -c"
  },
  "devDependencies": {
    "@typhonjs-build-test/esm-d-ts": "^0.3.0-next.7",
    "@typhonjs-typedoc/typedoc-pkg": "^0.0.5",
    "@typhonjs-utils/file-util": "^0.1.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "esbuild": "^0.21.0",
    "typedoc": "0.25.12",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
